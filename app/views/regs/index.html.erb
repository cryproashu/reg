<h1>Listing regs</h1>

<table>
  <tr>
    <th>Fname</th>
    <th>Lname</th>
    <th>Email</th>
    <th>Background</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @regs.each do |reg| %>
  <tr>
    <td><%= reg.fname %></td>
    <td><%= reg.lname %></td>
    <td><%= reg.email %></td>
    <td><%= reg.background %></td>
    <td><%= link_to 'Show', reg %></td>
    <td><%= link_to 'Edit', edit_reg_path(reg) %></td>
    <td><%= link_to 'Destroy', reg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Reg', new_reg_path %>
